<Page
    x:Class="Footap.AddFood"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Footap"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="360" Margin="80,85,0,0" VerticalAlignment="Top" Width="240"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="90" Margin="400,80,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="240"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="65" Margin="440,235,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="button_Copy" Content="Button" HorizontalAlignment="Left" Height="65" Margin="440,350,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="button_Copy1" Content="Button" HorizontalAlignment="Left" Height="65" Margin="440,475,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="button_Copy2" Content="Button" HorizontalAlignment="Left" Height="65" Margin="1090,475,0,0" VerticalAlignment="Top" Width="115"/>
        <ListView x:Name="listView_Copy" HorizontalAlignment="Left" Height="360" Margin="800,85,0,0" VerticalAlignment="Top" Width="240"/>

    </Grid>
</Page>
